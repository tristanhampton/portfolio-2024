<main class="site__main main--projects">

	<h1>Projects</h1>

	<h2>Projects with Yellow Pencil</h2>
	<small><em>February 2022 - Present</em></small>
	<ul class="project-list">
		{% for project in collections.yellowpencil | reverse %}
			{% if project.data.published != false %}
				<li class="project-item">
					<a href="{{ project.data.website_url if project.data.redirect else project.url }}" title="{{ project.data.title }}" target="{{ '_blank' if project.data.redirect else '' }}" rel="noopener noreferrer">
						<h3>{{ project.data.short }}</h3>
						<picture>
							<img src="/projects/img/{{ project.data.img_small }}" alt="screenshot of the {{ project.data.title }} website">
						</picture>
					</a>
				</li>
			{% endif %}
		{% endfor %}
	</ul>

	<h2>Projects with Box Clever</h2>
	<small><em>December 2020 - February 2022</em></small>
	<ul class="project-list">
		{% for project in collections.boxclever | reverse %}
			{% if project.data.published != false %}
				<li class="project-item">
					<a href="{{ project.data.website_url if project.data.redirect else project.url }}" title="{{ project.data.title }}" target="{{ '_blank' if project.data.redirect else '' }}" rel="noopener noreferrer">
						<h3>{{ project.data.short }}</h3>
						<picture>
							<img src="/projects/img/{{ project.data.img_small }}" alt="screenshot of the {{ project.data.title }} website">
						</picture>
					</a>
				</li>
			{% endif %}
		{% endfor %}
	</ul>
</main>